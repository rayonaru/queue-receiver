{
  "ConnectionStrings": {
    "Develop": ""
  },
  "Queue": {
    "HostName": "",
    "Port": "",
    "UserName": "",
    "Password": "",
    "VirtualHost": ""
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "D:\\Logs\\QueueReceiver\\Prod\\${shortdate}\\${logger}_Log.log",
        "layout": "----------------------------------------${newline}${longdate} |${level}| ${message}${newline}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logfile"
      }
    ]
  }
}
